<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>


<AnchorPane xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.alfaintegral.deviceProduction.controllers.ProfileSceneController">
   <children>
      <BorderPane prefHeight="448.0" prefWidth="624.0">
         <top>
            <ImageView fx:id="avatarImageView" fitHeight="100.0" fitWidth="120.0" pickOnBounds="true" preserveRatio="true" BorderPane.alignment="CENTER">
               <BorderPane.margin>
                  <Insets top="25.0" />
               </BorderPane.margin>
            </ImageView>
         </top>
         <center>
            <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
               <children>
                  <Label fx:id="usernameLabel" prefHeight="28.0" prefWidth="199.0" text="Username" />
                  <Label fx:id="nameLabel" text="Name" />
                  <Label fx:id="surnameLabel" text="Surname" />
                  <Label fx:id="middleNameLabel" layoutX="10.0" layoutY="105.0" text="MiddleName" />
                  <Label fx:id="mailLabel" layoutX="10.0" layoutY="72.0" text="Mail" />
               </children>
            </VBox>
         </center>
      </BorderPane>
   </children>
</AnchorPane>
